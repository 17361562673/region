<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>loadPro</servlet-name>
        <servlet-class>controller.LoadProServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loadPro</servlet-name>
        <url-pattern>/loadPro</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>loadOp</servlet-name>
        <servlet-class>controller.LoadOpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>loadOp</servlet-name>
        <url-pattern>/loadOp</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>loadPro</welcome-file>
    </welcome-file-list>
</web-app>